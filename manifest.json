{
  "manifest_version": 3,
  "name": "Sketchテニス予約",
  "version": "1.0",
  "description": "Sketchのテニスコートを予約するための拡張機能です。",
  "action": {},
  "permissions": [
    "storage",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://kouen.sports.metro.tokyo.lg.jp/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
